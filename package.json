{
  "name": "pinecone-client",
  "version": "1.1.0",
  "description": "A React front end for Pinecone.",
  "main": "webpack.config.js",
  "scripts": {
    "test:webdriver:start": "webdriver-manager start",
    "test": "babel-node run.js",
    "client:test": "echo \"Error: no test specified\" && exit 1",
    "client:start:dev": "webpack-dev-server --hot --inline",
    "client:package": "webpack",
    "preclient:docker:build:dev": "npm run client:package",
    "client:docker:run:dev": "docker run --detach --name pinecone-client -p 80:80 --link pinecone-server:server registry.gitlab.com/reallybigtree/pinecone-client",
    "client:docker:build:dev": "docker build -t registry.gitlab.com/reallybigtree/pinecone-client .",
    "client:docker:push": "docker push registry.gitlab.com/reallybigtree/pinecone-client",
    "webdriver-update": "webdriver-manager update",
    "client:update-webdriver": "webdriver-manager update"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.12.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "bluebird": "^3.4.1",
    "css-loader": "^0.23.1",
    "flux": "^2.1.1",
    "history": "^3.0.0",
    "jasmine": "^2.4.1",
    "moment": "^2.14.1",
    "mongoose": "^4.5.4",
    "passport-local-mongoose": "^4.0.0",
    "pinecone-models": "git+ssh://git@gitlab.com/ReallyBigTree/Pinecone-Models.git#develop",
    "react": "^15.2.0",
    "react-datetime": "^2.3.2",
    "react-dom": "^15.2.0",
    "react-router": "^2.5.2",
    "style-loader": "^0.13.1",
    "wdio-jasmine-framework": "^0.2.3",
    "webdriver-manager": "^10.2.1",
    "webdriverio": "^4.2.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "wdio-jasmine-framework": "^0.2.3"
  }
}