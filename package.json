	"pinecone:client:start" : "webpack-dev-server --hot --inline",
	"pinecone:client:test" : "wdio wdio.conf.js",
	"pinecone:client:test:webdriver:start" : "webdriver-manager start",
	"pinecone:client:update-webdriver" : "webdriver-manager update",
	"prepinecone:client:docker:build" : "npm run pinecone:client:package",
	"test" : "npm run pinecone:client:test"
